extends ../base.pug

block variables
	- var template = 'about'

block content
	.about
		.about__wrapper
			section.about__gallery
				.about__gallery__wrapper
					each media in about.data.gallery
						figure.about__gallery__media
							img.about__gallery__media__image(alt=media.image.alt src=media.image.url width=media.image.dimensions.width/4 height=media.image.dimensions.height/4)
